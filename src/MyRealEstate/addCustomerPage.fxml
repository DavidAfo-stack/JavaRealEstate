<AnchorPane prefHeight="680.0" prefWidth="750.0" styleClass="Background" stylesheets="@estate.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MyRealEstate.addCustomerPageController">

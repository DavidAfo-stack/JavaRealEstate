         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button fx:id="customerSubmitButton" layoutX="313.0" layoutY="612.0" mnemonicParsing="false" onAction="#customerSubmitBtnListener" styleClass="HomeButtons" stylesheets="@estate.css" text="Submit" textFill="#1a870a" />
      <TextArea fx:id="presentAddrField" layoutX="17.0" layoutY="349.0" prefHeight="86.0" prefWidth="250.0" />
